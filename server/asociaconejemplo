  Mallas.associate = function(models) {
    // associations can be defined here
    Mallas.belongsTo(models.Personas)
  };
    Carreras.associate = function(models) {
    // associations can be defined here
    Carreras.hasMany(models.Mallas)
  };

    Institutos.associate = function(models) {
    // associations can be defined here
    Institutos.belongsTo(models.Emails)
  };
    Emails.associate = function(models) {
    // associations can be defined here
    Emails.hasMany(models.Institutos)
  };